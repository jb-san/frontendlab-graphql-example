{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "start",
      "problemMatcher": {
        "owner": "javascript",
        "fileLocation": ["relative", "${workspaceFolder}"],
        "pattern": [
          {
            "regexp": "(.*)",
            "file": 1,
            "line": 1,
            "severity": 1,
            "message": 1
          }
        ]
      }
    }
  ]
}
